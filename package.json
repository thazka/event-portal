{
  "name": "event-portal",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "run-s build:update-bulma-colors build:client",
    "build:update-bulma-colors": "bulma-css-vars",
    "build:client": "cross-env NODE_ENV=production vite build",
    "start": "cross-env NODE_ENV=production serve dist -s -p 5000",
    "test": "run-p test:*",
    "test:tsc": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@cssninja/bulma-css-vars": "0.9.2",
    "@fontsource-variable/fira-code": "5.0.18",
    "@fontsource-variable/montserrat": "5.0.19",
    "@fontsource-variable/roboto-flex": "5.0.15",
    "@unhead/addons": "1.9.9",
    "@unhead/ssr": "1.9.9",
    "@unhead/vue": "1.9.9",
    "@vueuse/core": "10.9.0",
    "@vueuse/router": "10.9.0",
    "bulma": "npm:@cssninja/bulma@0.9.4",
    "canvas-confetti": "^1.9.3",
    "cross-env": "7.0.3",
    "defu": "6.1.4",
    "focus-trap": "7.5.4",
    "focus-trap-vue": "4.0.3",
    "iconify-icon": "2.1.0",
    "npm-run-all": "4.1.5",
    "pinia": "2.1.7",
    "serve": "14.2.3",
    "tslib": "2.6.2",
    "ufo": "1.5.3",
    "unhead": "1.9.9",
    "unplugin-vue-router": "0.8.6",
    "vee-validate": "4.12.6",
    "vue": "3.4.26",
    "vue-router": "4.3.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "20.12.8",
    "@vitejs/plugin-vue": "5.0.4",
    "rollup-plugin-purgecss": "6.0.0",
    "sass": "npm:sass-embedded@1.75.0",
    "terser": "5.31.0",
    "typescript": "5.4.5",
    "unplugin-auto-import": "0.17.5",
    "unplugin-vue-components": "0.27.0",
    "vite": "5.2.11",
    "vite-plugin-vue-devtools": "7.1.3",
